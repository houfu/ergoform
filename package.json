{
  "name": "ergoform",
  "version": "0.8.0",
  "description": "ergoform provides a framework for converting JSON objects into HTML forms",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "test": "ts-node --project \"./tsconfig.json\" -r tsconfig-paths/register node_modules/jasmine/bin/jasmine",
    "build": "tsc",
    "build:watch": "tsc --watch",
    "lint": "tslint --project \"./tsconfig.json\"",
    "docs": "typedoc --out docs",
    "antlr4ts-selectitems": "antlr4ts -visitor src/grammars/SelectItems.g4",
    "antlr4ts-selectboxexpress": "antlr4ts -visitor src/grammars/SelectBoxExpress.g4"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/houfu/ergoform.git"
  },
  "author": "Houfu Ang <houfu@outlook.sg>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/houfu/ergoform/issues"
  },
  "homepage": "https://github.com/houfu/ergoform#readme",
  "devDependencies": {
    "@types/jasmine": "^2.8.9",
    "antlr4ts-cli": "^0.4.0-alpha.4",
    "jasmine": "^3.2.0",
    "ts-node": "^7.0.1",
    "tsconfig-paths": "^3.6.0",
    "tslint": "^5.11.0",
    "typedoc": "^0.13.0",
    "typescript": "^3.1.3"
  },
  "dependencies": {
    "antlr4": "^4.7.1",
    "antlr4ts": "^0.4.1-alpha.0"
  }
}
